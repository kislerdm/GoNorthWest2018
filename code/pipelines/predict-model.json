{
  "pipeline": {
    "name": "prediction"
  },
  "transform": {
    "image": "pskreter/predict-req:1.0.0",
    "cmd": [
      "/predict",
      "-modelDir=/pfs/model",
      "-varDir=/pfs/attributes",
      "-outDir=/pfs/out"
    ]
  },
  "parallelism_spec": {
    "constant": "1"
  },
  "input": {
    "cross": [
      {
        "atom": {
          "repo": "attributes",
          "glob": "/*"
        }
      },
      {
        "atom": {
          "repo": "model",
          "glob": "/"
        }
      }
    ]
  }
}
